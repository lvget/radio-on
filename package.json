{
  "name": "radio",
  "version": "0.0.1",
  "description": "Collection Online Radio stations",
  "productName": "Radio On",
  "author": "lost <l.ostanin@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m pwa",
    "build-pwa": "quasar build -m pwa",
    "firebase:deploy": "firebase deploy --only hosting:radio-on",
    "firebase:deploy:test": "firebase hosting:channel:deploy test",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "icongenie": "icongenie generate -m pwa --icon src/assets/app-icon.png",
    "git-push": "git add . && git commit -m \"update\" && git push origin main",
    "git-pull": "git pull origin main",
    "git-status": "git status",
    "git-stash": "git stash",
    "git-pop": "git stash pop",
    "git-reset": "git fetch && git reset --hard origin/main && git clean -fd"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "chardet": "^2.1.0",
    "firebase": "^10.11.1",
    "icecast-metadata-js": "^1.2.8",
    "icecast-metadata-player": "^1.17.2",
    "icecast-metadata-stats": "^0.1.11",
    "pinia": "^2.1.7",
    "quasar": "^2.8.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@quasar/app-vite": "^1.8.0",
    "@types/chardet": "^0.8.3",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "autoprefixer": "^10.4.2",
    "axios": "^1.13.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^27.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite-plugin-checker": "^0.6.4",
    "vue-tsc": "^1.8.22",
    "workbox-build": "^7.1.0",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
